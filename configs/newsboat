# ~/.config/newsboat/config

notify-program "notify-send"
auto-reload yes
reload-threads 100

# unbind keys
unbind-key j
unbind-key k
unbind-key J
unbind-key K

# bind keys - vim style
bind-key j down
bind-key k up
bind-key l open
bind-key h quit

include /usr/share/doc/newsboat/contrib/colorschemes/inkpot
